<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FormDaftarCheckUp">

    <Spinner
        android:id="@+id/spinner3"
        android:layout_width="320dp"
        android:layout_height="32dp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="230dp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pilih Poli"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="189dp" />

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="match_parent"
        android:layout_height="64dp"
        android:background="#DB4062"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="1dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Daftar Checkup"
        android:textColor="#FBF9F9"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pilih Rumah Sakit"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="98dp" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/button4"
        android:layout_width="320dp"
        android:layout_height="48dpdp"
        android:text="Lihat Jadwal"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="644dp"
        app:backgroundTint="#DB4062"/>

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="320dp"
        android:layout_height="32dp"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="139dp" />

</androidx.constraintlayout.widget.ConstraintLayout>